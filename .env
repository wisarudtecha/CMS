# Test/Staging environment variables for a CMS application

# Base URL Configuration
VITE_BASE_URL=https://dtacwsproxy.welcome-ddns.com
# VITE_BASE_URL=https://demo.welcomedcc.com

# API Configuration
VITE_API_BASE_URL=https://dtacwsproxy.welcome-ddns.com/api/v1
# VITE_API_BASE_URL=https://demo.welcomedcc.com/api/v1
VITE_GET_CASE_PER_REQUEST=500

# Websocket Configuration
VITE_WS_URL=wss://dtacwsproxy.welcome-ddns.com/ws
VITE_WEBSOCKET_BASE_URL=wss://dtacwsproxy.welcome-ddns.com

# Allowed hosts (comma-separated)
VITE_ALLOWED_HOSTS=0.0.0.0,127.0.0.1,*.welcome-ddns.com,dtacwsproxy.welcome-ddns.com,localhost

# HMR Host
VITE_HMR_HOST=dtacwsproxy.welcome-ddns.com

# Development Settings
VITE_MOCK_API=false
VITE_API_DELAY=0
VITE_ENABLE_DEVTOOLS=true

# Authentication
VITE_JWT_SECRET=your-jwt-secret-here
VITE_TOKEN_EXPIRY=3600

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME=true
VITE_UTC_TIMEZONE=-7
