# Production environment variables for a CMS application
#VITE_API_BASE_URL="https://cms-api-1-production.up.railway.app/api/v1"
#VITE_WEBSOCKET_BASE_URL="wss://cms-api-1-production.up.railway.app"

# API Configuration
VITE_API_BASE_URL=https://cms-api-1-production.up.railway.app/api/v1
VITE_WS_URL=wss://cms-api-1-production.up.railway.app/ws

# Development Settings
VITE_MOCK_API=false
VITE_API_DELAY=0
VITE_ENABLE_DEVTOOLS=true

# Authentication
VITE_JWT_SECRET=your-jwt-secret-here
VITE_TOKEN_EXPIRY=3600

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME=true
